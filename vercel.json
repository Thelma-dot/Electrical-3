{
  "version": 2,
  "builds": [
    {
      "src": "Backend/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/Backend/api/index.js"
    },
    {
      "src": "/health",
      "dest": "/Backend/api/index.js"
    },
    {
      "src": "/test",
      "dest": "/Backend/api/index.js"
    },
    {
      "src": "/images/(.*)",
      "dest": "/Frontend/images/$1"
    },
    {
      "src": "/(.*\\.(css|js|png|jpg|jpeg|gif|svg|ico|mp3|wav))",
      "dest": "/Frontend/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "/Frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/Frontend/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
